{
  "expo": {
    "name": "Transporte Madrid",
    "description": "Transport app",
    "slug": "transporte",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": ["ios", "android"],
    "version": "0.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "versionCode": 5,
      "package": "com.transport.rn",
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "permissions": []
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "javoto17",
            "project": "transport-madrid",
            "authToken": "",
          }
        }
      ]
    }
  }
}
